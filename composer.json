{
    "name": "designcrystals/basecontroller",
    "description": "Provides the basic functionalities of a resource controller out-of-the-box.",
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Design Crystals",
            "email": "dc.designcrystals@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {},
    "autoload": {
        "psr-4": {
            "DesignCrystals\\BaseController\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DesignCrystals\\BaseController\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DesignCrystals\\BaseController\\BaseControllerServiceProvider"
            ],
            "aliases": {
                "Base": "DesignCrystals\\BaseController\\Facades\\Base"
            }
        }
    }
}
